{
  "name": "netlify-faunadb-crud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "serve": "npm-run-all --parallel serve:app serve:server",
    "serve:app": "vue-cli-service serve",
    "serve:server": "netlify-lambda serve src/functions -c ./webpack.config.js",
    "prebuild": "echo 'setup faunaDB' && yarn bootstrap",
    "build": "npm-run-all --parallel build:app build:functions",
    "build:app": "vue-cli-service build",
    "build:functions": "netlify-lambda build src/functions -c ./webpack.config.js"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "faunadb": "^2.14.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.0.0",
    "encoding": "^0.1.12",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "netlify-lambda": "^1.6.3",
    "readline": "^1.3.0",
    "vue-template-compiler": "^2.6.11",
    "yarn-run-all": "^3.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
